<helpdocument version="1.0">
  <meta>
    <topic id="textsimpress00slidemenuxml" indexer="exclude" status="PUBLISH">
      <title id="tit" xml-lang="en-US">Slide Menu</title>
      <filename>/text/simpress/00/slide_menu.xhp</filename>
    </topic>
  </meta>
  <body>
    <paragraph role="paragraph" id="par_id3155530" xml-lang="en-US"><variable id="frtite">Choose <menuitem>Slide - Slide Properties</menuitem>
    </variable></paragraph>
    <paragraph role="paragraph" id="par_id3145386" xml-lang="en-US"><variable id="frtites">Choose <menuitem>Slide - Slide Properties</menuitem> and then click the <emph>Page</emph> tab
    </variable></paragraph>
    <paragraph role="paragraph" id="par_id3148866" xml-lang="en-US"><variable id="frtiteh">Choose <menuitem>Slide - Slide Properties</menuitem> and then click the <emph>Background</emph> tab
    </variable></paragraph>
    <paragraph role="paragraph" id="par_id3155266" xml-lang="en-US"><variable id="adnsei">Choose <menuitem>Slide - Layout</menuitem>
    </variable></paragraph>
    <paragraph role="paragraph" id="par_id3153012" xml-lang="en-US"><variable id="seitenvorlage">Choose <menuitem>Slide - Change Slide Master</menuitem>
    </variable></paragraph>
    <paragraph role="paragraph" id="par_id911634055593609"><variable id="loadslidedesign">Choose <menuitem>Slide - Change Slide Master - Load</menuitem></variable></paragraph>
    <section id="new_slide">
      <paragraph id="par_id3134264" role="paragraph" xml-lang="en-US">Choose <menuitem>Slide - New Slide</menuitem></paragraph>
      <paragraph id="par_id3177597" role="paragraph" xml-lang="en-US">On the <emph>Presentation</emph> bar, click</paragraph>
      <section id="insertslide">
        <table id="tbl_id3149334">
          <tablerow>
            <tablecell>
              <paragraph id="par_id3685251" role="paragraph" xml-lang="en-US"> <image id="img_id3183073" src="cmd/sc_insertpage.png" width="1cm" height="1cm"><alt xml-lang="en-US" id="alt_id3183073">Icon New slide</alt></image></paragraph>
            </tablecell>
            <tablecell>
              <paragraph id="par_id7354512" role="paragraph" xml-lang="en-US">New Slide</paragraph>
            </tablecell>
          </tablerow>
        </table>
      </section>
    </section>
  </body>
</helpdocument>
