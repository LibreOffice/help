<tablerow>
<tablecell colspan="" rowspan="">
<paragraph xml-lang="en-US" id="hd_ii3154718" role="tablecontent" l10n="U" oldref="142">Arrow Up</paragraph>
</tablecell>
<tablecell colspan="" rowspan="">
<paragraph xml-lang="en-US" id="par_ii3154731" role="tablecontent" l10n="CHG" oldref="143">Move cursor up one line</paragraph>
</tablecell>
</tablerow>
