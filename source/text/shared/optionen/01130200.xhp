<meta>
      <topic id="textsharedoptionen01130200xml" indexer="include" status="PUBLISH">
         <title xml-lang="en-US" id="tit">Microsoft Office</title>
         <filename>/text/shared/optionen/01130200.xhp</filename>
      </topic>
