<paragraph role="tablecontent" id="par_id3149344" xml-lang="en-US"><switchinline select="sys"><caseinline select="UNIX">{install}/share/classification/example.xml</caseinline><caseinline select="MAC">{install}/share/classification/example.xml</caseinline><defaultinline>{install}\share\classification\example.xml</defaultinline></switchinline></paragraph>
