<paragraph role="tablecontent" id="par_id3149343" xml-lang="en-US"><switchinline select="sys"><caseinline select="UNIX">{user profile}/user/temp
</caseinline><caseinline select="MAC">{user profile}/user/temp
</caseinline><defaultinline>{user profile}\user\temp</defaultinline></switchinline></paragraph>
