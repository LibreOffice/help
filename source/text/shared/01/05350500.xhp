        <tablecell>
            <paragraph id="par_id871592540872026" role="paragraph">
                <image src="svx/res/3dtextur.png" id="img_id241592540872027" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id151592540872028">Icon Textures</alt></image>
            </paragraph>
        </tablecell>
        <tablecell>
            <paragraph id="par_id41592540872029" role="paragraph" xml-lang="en-US" >Textures</paragraph>
        </tablecell>
