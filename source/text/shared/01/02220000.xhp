<bookmark xml-lang="en-US" branch="hid/svx/ui/imapdialog/TBI_POLYDELETE" id="bm_id3145642" localize="false"/>

<paragraph id="hd_id3083283" role="heading" level="2" xml-lang="en-US">Delete Points</paragraph>
<paragraph id="par_id3163824" role="paragraph" xml-lang="en-US"><ahelp hid="svx/ui/imapdialog/TBI_POLYDELETE">Deletes the selected anchor point.</ahelp></paragraph>

<table id="tbl_id3159183">
