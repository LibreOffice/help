    <tablecell>
      <paragraph id="par_id3153839" role="paragraph" xml-lang="en-US">Move Points</paragraph>
    </tablecell>
  </tablerow>
</table>

<bookmark xml-lang="en-US" branch="hid/svx/ui/imapdialog/TBI_POLYINSERT" id="bm_id3153532" localize="false"/>

<paragraph id="hd_id3145162" role="heading" level="2" xml-lang="en-US">Insert Points</paragraph>
<paragraph id="par_id3156355" role="paragraph" xml-lang="en-US"><ahelp hid="svx/ui/imapdialog/TBI_POLYINSERT">Adds an anchor point where you click on the outline of the hotspot.</ahelp></paragraph>

<table id="tbl_id3154479">
