    </tablecell>
  </tablerow>
</table>

<bookmark xml-lang="en-US" branch="hid/svx/ui/imapdialog/TBI_ACTIVE" id="bm_id3156089" localize="false"/>

<paragraph id="hd_id3166448" role="heading" level="2" xml-lang="en-US">Active</paragraph>
<paragraph id="par_id3146918" role="paragraph" xml-lang="en-US"><ahelp hid="svx/ui/imapdialog/TBI_ACTIVE">Disables or enables the hyperlink for the selected hotspot. A disabled hotspot is transparent.</ahelp></paragraph>

<table id="tbl_id3146317">
  <tablerow>
    <tablecell>
      <paragraph id="par_id3155901" role="paragraph" xml-lang="en-US"> <image id="img_id3145232" src="svx/res/id016.png"><alt xml-lang="en-US" id="alt_id3145232">Icon</alt></image></paragraph>
    </tablecell>
    <tablecell>
      <paragraph id="par_id3155959" role="paragraph" xml-lang="en-US">Active</paragraph>
    </tablecell>
  </tablerow>
</table>

<bookmark xml-lang="en-US" branch="hid/svx/ui/imapdialog/TBI_MACRO" id="bm_id3151186" localize="false"/>

<paragraph id="hd_id3153966" role="heading" level="2" xml-lang="en-US">Macro</paragraph>
<paragraph id="par_id3151250" role="paragraph" xml-lang="en-US"><ahelp hid="svx/ui/imapdialog/TBI_MACRO">Lets you assign a macro that runs when you click the selected hotspot in a browser.</ahelp></paragraph>

<table id="tbl_id3149926">
