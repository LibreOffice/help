    <tablecell>
      <paragraph id="par_id3153416" role="paragraph" xml-lang="en-US">Edit points</paragraph>
    </tablecell>
  </tablerow>
</table>

<bookmark xml-lang="en-US" branch="hid/svx/ui/imapdialog/TBI_POLYMOVE" id="bm_id3159112" localize="false"/>

<paragraph id="hd_id3155600" role="heading" level="2" xml-lang="en-US">Move Points</paragraph>
<paragraph id="par_id3151318" role="paragraph" xml-lang="en-US"><ahelp hid="svx/ui/imapdialog/TBI_POLYMOVE">Lets you move the individual anchor points of the selected hotspot.</ahelp></paragraph>

