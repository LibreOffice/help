        </case>
        <case select="CALC">
            <embed href="text/shared/00/00000004.xhp#from_menu_bar"/>
            <paragraph role="paragraph" id="par_id371702933232002">On a cell with no comment, choose <menuitem>Insert - Comment</menuitem>.</paragraph>
            <paragraph role="paragraph" id="par_id311702929340458">On a cell with existing comment, choose <menuitem>Sheet - Cell Comments - Edit Comment</menuitem>.</paragraph>
            <embed href="text/shared/00/00000004.xhp#from_tabUI"/>
