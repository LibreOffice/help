                <paragraph id="par_id9149694" role="paragraph">Choose <menuitem>Page - Properties - Background</menuitem> tab.</paragraph>
                <paragraph role="paragraph" id="par_id471592158481451">Choose <menuitem>Format - Area - Area</menuitem> tab</paragraph>
            </case>
            <case select="CHART">
                <paragraph id="par_id3152922" role="paragraph">Choose <menuitem>Format - Title - Area</menuitem> tab.</paragraph>
                <paragraph id="par_id3157894" role="paragraph">Choose <menuitem>Format - Legend - Area</menuitem> tab.</paragraph>
                <paragraph id="par_id3144444" role="paragraph">Choose <menuitem>Format - Chart Wall - Area</menuitem> tab.</paragraph>
                <paragraph id="par_id3156543" role="paragraph">Choose <menuitem>Format - Chart Floor - Area</menuitem>.</paragraph>
                <paragraph id="par_id3150685" role="paragraph">Choose <menuitem>Format - Chart Area - Area</menuitem>.</paragraph>
