    <h1 id="hd_id3149741">Format Menu</h1>
    <section id="linie1">
        <paragraph id="par_id3146857" role="paragraph">Choose <emph>Format - Line</emph> (Impress and Draw).</paragraph><comment>i64150</comment>
        <paragraph id="par_id366527" role="paragraph">Choose <emph>Format - Text Box and Shape - Line</emph> (Writer).</paragraph>
        <paragraph id="par_id3835261" role="paragraph">Choose <emph>Format - Object - Line</emph> (Calc).</paragraph>
        <paragraph id="par_id3148668" role="paragraph">On <emph>Line and Filling</emph> bar, click</paragraph>
        <section id="sylinie">
            <table id="tbl_id3145160">
                <tablerow>
                    <tablecell>
                        <paragraph id="par_id3148889" role="paragraph"><image id="img_id3150669" src="cmd/sc_formatline.svg" width="1cm" height="1cm"><alt id="alt_id3150669">Icon Line</alt></image></paragraph>
