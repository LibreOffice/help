<bookmark xml-lang="en-US" branch="hid/cui/ui/hyperlinkdialog/icon_control" id="bm_id3149177" localize="false"/>
<paragraph xml-lang="en-US" id="par_id3155391" role="paragraph"><ahelp hid=".">Select the type of hyperlink to be inserted.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:OpenHyperlinkOnCursor" id="bm_id7868083" localize="false"/>
<paragraph xml-lang="en-US" id="par_id3153683" role="paragraph"><ahelp hid=".uno:OpenHyperlinkOnCursor" visibility="hidden">Opens the hyperlink in your default web browser.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:EditHyperlink" id="bm_id0122200902201484" localize="false"/>
<paragraph xml-lang="en-US" id="par_id0122200902231573" role="paragraph"><ahelp hid="." visibility="hidden">Opens the <emph>Hyperlink</emph> dialog.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:CopyHyperlinkLocation" id="bm_id0122200902224858" localize="false"/>
<paragraph xml-lang="en-US" id="par_id0122200902231660" role="paragraph"><ahelp hid="." visibility="hidden">Copies the URL to the clipboard.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:RemoveHyperlink" id="bm_id0122200902230774" localize="false"/>
<paragraph xml-lang="en-US" id="par_id0122200902231630" role="paragraph"><ahelp hid="." visibility="hidden">Removes the hyperlink, leaving plain text.</ahelp></paragraph>
