<bookmark xml-lang="en-US" branch="hid/cui/ui/hyperlinkdialog/apply" id="bm_id3149811" localize="false"/>
<h3 xml-lang="en-US" id="hd_id3166410">Apply</h3>
<paragraph xml-lang="en-US" id="par_id3147209" role="paragraph"><ahelp hid=".">Applies the data to your document.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui/ui/hyperlinkdialog/cancel" id="bm_id3159166" localize="false"/>
<h3 xml-lang="en-US" id="hd_id3149398">Close</h3>
<paragraph xml-lang="en-US" id="par_id3149734" role="paragraph"><ahelp hid=".">Closes the dialog without saving.</ahelp></paragraph>
<h3 xml-lang="en-US" id="hd_id3153700">Help</h3>
<paragraph xml-lang="en-US" id="par_id3150943" role="paragraph">Opens the Help.</paragraph>
<bookmark xml-lang="en-US" branch="hid/cui/ui/hyperlinkdialog/reset" id="bm_id3155434" localize="false"/>
<h3 xml-lang="en-US" id="hd_id3156192">Reset</h3>
<paragraph xml-lang="en-US" id="par_id3149234" role="paragraph"><ahelp hid=".">Resets the entries in the dialog to their original state.</ahelp></paragraph>
