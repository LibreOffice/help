<paragraph role="heading" id="par_idN1054F" xml-lang="en-US" level="2" l10n="NEW">Other</paragraph>
<paragraph role="paragraph" id="par_idN10553" xml-lang="en-US" l10n="NEW"><ahelp hid="34291">Opens a file open dialog to select a background image file for the index page.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/WIZARDS_HID_BG_BTN_NONE" id="bm_id3982360" localize="false"/>
<paragraph role="heading" id="par_idN10556" xml-lang="en-US" level="2" l10n="NEW">None</paragraph>
