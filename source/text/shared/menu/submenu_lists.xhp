  <body>
    <section id="submenu_lists">
      <bookmark branch="hid/.uno:FormatBulletsMenu" id="bm_id335519659183789" localize="false"/>
      <h1 id="hd_id411816022675979">
        <link href="text/shared/menu/submenu_lists.xhp">Lists</link>
      </h1>
      <paragraph role="paragraph" id="par_id398855439580084">
        <ahelp hid=".">Opens a submenu where you can choose commands to create lists.</ahelp>
      </paragraph>
    </section>
    <section id="howtoget">
      <embed href="text/shared/00/00000004.xhp#from_menu_bar"/>
      <paragraph role="paragraph" id="par_id321690379868876">Choose <menuitem>Format - Lists</menuitem></paragraph>
    </section>

    <h2 id="hd_id3147573">Unordered List</h2>
    <paragraph role="paragraph" id="par_id261643820748505">Creates an unordered list from selection.</paragraph>
    <table id="tab_id621643820768224">
      <tablerow>
        <tablecell>
          <paragraph id="par_id261643820768225" role="tablecontent">
            <image src="cmd/lc_defaultbullet.svg" id="img_id901643820768226" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id831643820768227">Icon Unordered List</alt></image>
          </paragraph>
        </tablecell>
        <tablecell>
          <paragraph id="par_id131643820768228" role="tablecontent">Unordered List</paragraph>
        </tablecell>
      </tablerow>
    </table>

    <h2 id="hd_id3147528">Ordered List</h2>
    <paragraph role="paragraph" id="par_id261643820736974">Creates an ordered list from selection.</paragraph>
    <table id="tab_id621643820768147">
      <tablerow>
        <tablecell>
          <paragraph id="par_id261643820768478" role="tablecontent">
            <image src="cmd/lc_defaultnumbering.svg" id="img_id901643820768458" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id901643820768458">Icon Ordered List</alt></image>
          </paragraph>
        </tablecell>
        <tablecell>
          <paragraph id="par_id131690380480231" role="tablecontent">Ordered List</paragraph>
        </tablecell>
      </tablerow>
    </table>
 
    <h2 id="hd_id3147404">Demote</h2>
    <paragraph role="paragraph" id="par_id261643820735428">Demotes selected items in a list, moving them one level down in the list.</paragraph>
    <table id="tab_id621643820768317">
      <tablerow>
        <tablecell>
          <paragraph id="par_id261643820768002" role="tablecontent">
            <image src="cmd/lc_decrementlevel.svg" id="img_id901643820760487" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id901643820760487">Icon Demote</alt></image>
          </paragraph>
        </tablecell>
        <tablecell>
          <paragraph id="par_id131690380489457" role="tablecontent">Demote</paragraph>
        </tablecell>
      </tablerow>
    </table>

    <h2 id="hd_id3145746">Promote</h2>
    <paragraph role="paragraph" id="par_id261643820735017">Promotes selected items in a list, moving them one level up in the list.</paragraph>
    <table id="tab_id621643820768985">
      <tablerow>
        <tablecell>
          <paragraph id="par_id261643820766332" role="tablecontent">
            <image src="cmd/lc_incrementlevel.svg" id="img_id901643820763230" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id901643820763230">Icon Promote</alt></image>
          </paragraph>
        </tablecell>
        <tablecell>
          <paragraph id="par_id131690380480985" role="tablecontent">Promote</paragraph>
        </tablecell>
      </tablerow>
    </table>

    <h2 id="hd_id3145554">Move Down</h2>
    <paragraph role="paragraph" id="par_id261643820731478">Moves the selected items one position down in the list.</paragraph>
    <table id="tab_id621643820768032">
      <tablerow>
        <tablecell>
          <paragraph id="par_id261643820765107" role="tablecontent">
            <image src="cmd/lc_movedown.svg" id="img_id901643820767650" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id901643820763478">Icon Move Down</alt></image>
          </paragraph>
        </tablecell>
        <tablecell>
          <paragraph id="par_id131690380484128" role="tablecontent">Move Down</paragraph>
        </tablecell>
      </tablerow>
    </table>

    <h2 id="hd_id3145862">Move Up</h2>
    <paragraph role="paragraph" id="par_id261643820731052">Moves the selected items one position up in the list.</paragraph>
    <table id="tab_id621643820768354">
      <tablerow>
        <tablecell>
          <paragraph id="par_id261643820765745" role="tablecontent">
            <image src="cmd/lc_moveup.svg" id="img_id901643820763320" width="1cm"  height="1cm"><alt xml-lang="en-US" id="alt_id9016438207636448">Icon Move Up</alt></image>
          </paragraph>
        </tablecell>
        <tablecell>
          <paragraph id="par_id131690380484208" role="tablecontent">Move Up</paragraph>
        </tablecell>
      </tablerow>
    </table>

    </body>
