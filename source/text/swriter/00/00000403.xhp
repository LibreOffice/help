      <paragraph id="par_id3150932" role="paragraph" xml-lang="en-US"> <image id="img_id3150502" src="cmd/sc_controlcodes.png" width="1cm" height="1cm"><alt xml-lang="en-US" id="alt_id3150502">Formatting Marks Icon</alt></image></paragraph>
    </tablecell>
    <tablecell>
      <paragraph id="par_id3153716" role="paragraph" xml-lang="en-US">Formatting Marks</paragraph>
    </tablecell>
  </tablerow>
</table>

</section>
</section>
<section id="show_whitespace">
    <paragraph role="paragraph" id="par_id401629894593714">Choose <menuitem>View - Show Whitespace</menuitem>.</paragraph>
   <table id="tab_id131629893774664">
