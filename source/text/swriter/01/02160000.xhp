<bookmark xml-lang="en-US" branch="hid/modules/swriter/ui/indexentry/previous" id="bm_id3155091" localize="false"/>
<paragraph role="heading" id="hd_id3155080" xml-lang="en-US" level="2" l10n="U" oldref="22">Arrow to left</paragraph>
