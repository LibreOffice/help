<bookmark xml-lang="en-US" branch="hid/modules/swriter/ui/indexentry/next" id="bm_id3154628" localize="false"/>

<h2 id="hd_id3154617">Next entry</h2>
<paragraph id="par_id3154633" role="paragraph"><ahelp hid="modules/swriter/ui/indexentry/next">Jumps to the next index entry of the same type in the document.</ahelp></paragraph>

<table id="tbl_id3149640">
