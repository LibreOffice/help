    </tablecell>
    <tablecell>
      <paragraph id="par_id3156108" role="paragraph">Next entry (same name)</paragraph>
    </tablecell>
  </tablerow>
</table>

<bookmark xml-lang="en-US" branch="hid/modules/swriter/ui/indexentry/previous" id="bm_id3155091" localize="false"/>

<h2 id="hd_id3155080">Previous entry</h2>
<paragraph id="par_id3154327" role="paragraph"><ahelp hid="modules/swriter/ui/indexentry/previous">Jumps to the previous index entry of the same type in the document.</ahelp></paragraph>

<table id="tbl_id3150084">
