    </tablecell>
    <tablecell>
      <paragraph id="par_id3150550" role="paragraph">Previous entry (same name)</paragraph>
    </tablecell>
  </tablerow>
</table>

<bookmark xml-lang="en-US" branch="hid/modules/swriter/ui/indexentry/last" id="bm_id3147747" localize="false"/>

<h2 id="hd_id3147736">Next entry (same name)</h2>
<paragraph id="par_id3149829" role="paragraph"><ahelp hid="modules/swriter/ui/indexentry/last">Jumps to the next index entry with the same entry and type as the current index entry.</ahelp></paragraph>

<table id="tbl_id3149848">
