<section id="formatsparklinemenu">
  <embed href="text/shared/00/00000004.xhp#from_menu_bar"/>
  <paragraph role="paragraph" id="par_id321701545027940">Choose <menuitem>Format - Sparklines</menuitem>.</paragraph>
