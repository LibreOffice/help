        <embed href="text/shared/00/00000004.xhp#from_menu_bar"/>
        <paragraph role="paragraph" id="par_id3154625"><variable id="dndpa">Choose <emph>Insert - Pivot Table</emph>.</variable></paragraph>
        <paragraph role="paragraph" id="par_id641698842008451">Choose <menuitem>Data - Pivot Table - Insert or Edit</menuitem>.</paragraph>
        <embed href="text/shared/00/00000004.xhp#from_tabUI"/>
        <paragraph role="paragraph" id="par_id741698842068169">Choose <menuitem>Insert - Pivot Table</menuitem>.</paragraph>
        <paragraph role="paragraph" id="par_id71698842082244">Choose <menuitem>Data - Pivot Table</menuitem>.</paragraph>
