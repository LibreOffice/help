                <tablerow>
                    <tablecell>
                        <paragraph role="paragraph" id="par_id3145799">
                            <image id="img_id3149413" src="cmd/sc_datafilterautofilter.png" width="0.222inch" height="0.222inch"><alt id="alt_id3149413">Icon</alt></image></paragraph>
