        <table id="tab_id401698842094888">
            <tablerow>
                <tablecell>
                    <paragraph id="par_id401698842094889" role="paragraph"><image src="cmd/lc_datadatapilotrun.svg" id="img_id691698842094890" width="1cm"  height="1cm"><alt id="alt_id81698842094891">Icon Pivot Table</alt></image></paragraph>
