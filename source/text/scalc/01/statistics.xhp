    <section id="anova">
      <bookmark xml-lang="en-US" branch="index" id="bm_id02001">
        <bookmark_value>Analysis toolpack;analysis of variance</bookmark_value>
        <bookmark_value>analysis of variance;Analysis toolpack</bookmark_value>
