    <section id="standard">
        <bookmark branch="hid/.uno:NumberFormatStandard" id="bm_id3152996" localize="false"/>
        <h2 id="hd_id3149182"><link href="text/scalc/01/default_number_formats.xhp">Number format: General</link></h2>
        <paragraph role="paragraph" id="par_id3163802"><ahelp hid=".uno:NumberFormatStandard" visibility="visible">Applies the default number format to the selected cells.</ahelp></paragraph>
        <section id="howtoget">
            <embed href="text/shared/00/00000004.xhp#from_menu_bar"/>
            <paragraph role="paragraph" id="par_id701698931147179">Choose <menuitem>Format - Number Format - General</menuitem>.</paragraph>
            <embed href="text/scalc/00/00000405.xhp#number_aux"/>
            <embed href="text/shared/00/00000004.xhp#from_toolbars"/>
            <table id="tbl_id3156326">
                <tablerow>
                    <tablecell>
                        <paragraph role="paragraph" id="par_id3155922"><image src="cmd/lc_numberformatstandard.svg" id="img_id3156024" width="1cm" height="1cm"><alt id="alt_id3156024">Icon General Number Format</alt></image></paragraph>
                    </tablecell>
                    <tablecell>
                        <paragraph role="paragraph" id="par_id3153361">Number Format: General</paragraph>
                    </tablecell>
                </tablerow>
            </table>
            <embed href="text/shared/00/00000004.xhp#from_keyboard"/>
            <paragraph role="paragraph" id="par_id41698931229411"><switchinline select="sys"><caseinline select="MAC"><keycode>Command</keycode></caseinline><defaultinline><keycode>Ctrl</keycode></defaultinline></switchinline><keycode> + Shift + 6</keycode></paragraph>
        </section>
    </section>
