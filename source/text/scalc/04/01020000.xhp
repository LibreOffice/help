<helpdocument version="1.0">
  <meta>
    <topic id="textscalc0401020000xml" indexer="include" status="PUBLISH">
      <title id="tit" xml-lang="en-US">Shortcut Keys for Spreadsheets</title>
      <filename>/text/scalc/04/01020000.xhp</filename>
    </topic>
