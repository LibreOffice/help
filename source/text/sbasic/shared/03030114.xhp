<helpdocument version="1.0">
  <meta>
    <topic id="textsbasicshared03030114xml" indexer="include" status="PUBLISH">
      <title id="tit" xml-lang="en-US">CDateFromUnoTime Function [Runtime]</title>
      <filename>/text/sbasic/shared/03030114.xhp</filename>
    </topic>
    <history>
      <created date="2013-09-10T16:00:00">Lionel Elie Mamane</created>
