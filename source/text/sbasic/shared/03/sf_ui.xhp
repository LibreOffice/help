           <paragraph id="par_id161587913266162" role="tablecontent" localize="false">ActiveWindow</paragraph>
        </tablecell>
        <tablecell>
           <paragraph id="par_id651587913266754" role="tablecontent" xml-lang="en-US">Yes</paragraph>
        </tablecell>
