   <bookmark xml-lang="en-US" localize="false" branch="index" id="bm_id2315879865929">
      <bookmark_value>UI service;Minimize</bookmark_value>
   </bookmark>
   <h2 id="hd_id21158798659210" localize="false">Minimize</h2>
