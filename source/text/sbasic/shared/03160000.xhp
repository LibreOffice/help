<meta>
  <topic id="textsbasicshared03160000xml" indexer="include" status="PUBLISH">
     <title id="tit" xml-lang="en-US">Input Function [VBA]</title>
     <filename>/text/sbasic/shared/03160000.xhp</filename>
  </topic>
