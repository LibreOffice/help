<bookmark xml-lang="en-US" branch="index" id="bm_id3148585"><bookmark_value>cropping pictures</bookmark_value>
<bookmark_value>pictures; cropping</bookmark_value>
<bookmark_value>zooming; pictures</bookmark_value>
<bookmark_value>pictures; zooming</bookmark_value>
