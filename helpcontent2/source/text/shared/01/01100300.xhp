  <section id="benutzer">
  <bookmark branch="hid/HID_DOCINFOUSER" xml-lang="en-US" id="bm_id3154926"/><paragraph id="hd_id3155069" role="heading" level="1" oldref="1" l10n="U" xml-lang="en-US"><link href="text/shared/01/01100300.xhp" name="User">User</link></paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3155934" xml-lang="en-US" oldref="9"><ahelp hid="HID_DOCINFOUSER" visibility="visible">Allows you to assign custom information fields to your document.</ahelp></paragraph>
  </section>
  <section id="howtoget">
  <embed href="text/shared/00/00000401.xhp#info4"/>
</section>
  <bookmark branch="hid/SFX2:EDIT:TP_DOCINFOUSER:ED_INFO1" xml-lang="en-US" id="bm_id3154751"/><bookmark branch="hid/SFX2:EDIT:TP_DOCINFOUSER:ED_INFO2" xml-lang="en-US" id="bm_id3152425"/><bookmark branch="hid/SFX2:EDIT:TP_DOCINFOUSER:ED_INFO3" xml-lang="en-US" id="bm_id3148548"/><bookmark branch="hid/SFX2:EDIT:TP_DOCINFOUSER:ED_INFO4" xml-lang="en-US" id="bm_id3147399"/><paragraph role="heading" level="2" id="hd_id3151234" l10n="U" xml-lang="en-US" oldref="2">Info 1 to Info 4</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3152551" xml-lang="en-US" oldref="3"><ahelp visibility="visible" hid="SFX2:EDIT:TP_DOCINFOUSER:ED_INFO4">Enter your custom content. If you want, you can change the label for the content by clicking the <emph>Info Fields </emph>button.</ahelp></paragraph>
  <bookmark branch="hid/SFX2:PUSHBUTTON:TP_DOCINFOUSER:BTN_EDITLABEL" xml-lang="en-US" id="bm_id3145072"/><paragraph role="heading" level="2" id="hd_id3153528" l10n="U" xml-lang="en-US" oldref="4">Info Fields</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3157958" xml-lang="en-US" oldref="5"><ahelp hid="SFX2:PUSHBUTTON:TP_DOCINFOUSER:BTN_EDITLABEL" visibility="visible">Edit the labels for the custom content fields.</ahelp></paragraph>
  <bookmark branch="hid/HID_DOCINFO_EDT" xml-lang="en-US" id="bm_id3155355"/><paragraph role="heading" level="3" id="hd_id3155338" l10n="U" xml-lang="en-US" oldref="10">Edit Field Names</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3153311" xml-lang="en-US" oldref="11"><ahelp visibility="visible" hid="HID_DOCINFO_EDT">Allows you to modify the labels for the <emph>Info Fields</emph>.</ahelp></paragraph>
  <bookmark branch="hid/SFX2:EDIT:DLG_DOCINFO_EDT:ED_INFO1" xml-lang="en-US" id="bm_id3150443"/><bookmark branch="hid/SFX2:EDIT:DLG_DOCINFO_EDT:ED_INFO2" xml-lang="en-US" id="bm_id3156114"/><bookmark branch="hid/SFX2:EDIT:DLG_DOCINFO_EDT:ED_INFO3" xml-lang="en-US" id="bm_id3154749"/><bookmark branch="hid/SFX2:EDIT:DLG_DOCINFO_EDT:ED_INFO4" xml-lang="en-US" id="bm_id3152780"/><paragraph role="heading" level="3" id="hd_id3153031" l10n="U" xml-lang="en-US" oldref="6">Names</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3159166" xml-lang="en-US" oldref="7"><ahelp visibility="visible" hid="SFX2:EDIT:DLG_DOCINFO_EDT:ED_INFO4">Enter a name for the <emph>Info Field(s)</emph>.</ahelp></paragraph>
  <paragraph xml-lang="en-US" role="paragraph" id="par_id3153827" l10n="U" oldref="8">Click <emph>OK</emph> to apply your changes.</paragraph>
