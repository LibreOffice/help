  <bookmark branch="hid/HID_IMAPDLG_MACRO" xml-lang="en-US" id="bm_id3151186"/><paragraph role="heading" level="2" id="hd_id3153966" l10n="U" xml-lang="en-US" oldref="38">Macro</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3151250" xml-lang="en-US" oldref="40"><ahelp hid="HID_IMAPDLG_MACRO" visibility="visible">Lets you assign a macro that runs when you click the selected hotspot in a browser.</ahelp>  </paragraph>
  <table id="tbl_id3149926">
   
   
   
    <tablerow>
     <tablecell>
      <paragraph l10n="E" role="paragraph" id="par_id3145769" xml-lang="en-US" oldref=""><image src="res/sc30770.png" id="img_id3153922"><alt xml-lang="en-US" id="alt_id3153922">Icon</alt></image></paragraph>
     </tablecell>
     <tablecell>
