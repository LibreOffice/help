  <bookmark branch="hid/HID_IMAPDLG_ACTIVE" xml-lang="en-US" id="bm_id3156089"/><paragraph role="heading" level="2" id="hd_id3166448" l10n="U" xml-lang="en-US" oldref="26">Active</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3146918" xml-lang="en-US" oldref="28"><ahelp hid="HID_IMAPDLG_ACTIVE" visibility="visible">Disables or enables the hyperlink for the selected hotspot. A disabled hotspot is transparent.</ahelp></paragraph>
  <table id="tbl_id3146317">
   
   
   
