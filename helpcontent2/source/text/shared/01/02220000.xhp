      <paragraph l10n="E" role="paragraph" id="par_id3155901" xml-lang="en-US" oldref=""><image src="svx/res/id016.png" id="img_id3145232"><alt xml-lang="en-US" id="alt_id3145232">Icon</alt></image></paragraph>
