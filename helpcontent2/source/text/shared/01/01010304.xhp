  <section id="geschaeft">
  <paragraph id="hd_id3152942" role="heading" level="1" oldref="1" l10n="U" xml-lang="en-US"><link href="text/shared/01/01010304.xhp" name="Business">Business</link></paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3151097" xml-lang="en-US" oldref="2"><ahelp visibility="visible" hid="">Contains contact information for business cards that use a layout from a 'Business Card, Work' category. Business card layouts are selected on the <emph>Business Cards</emph> tab.</ahelp></paragraph>
  </section>
  <paragraph role="paragraph" id="par_id3150445" l10n="E" xml-lang="en-US" oldref="" localize="false"><link href="text/shared/00/00000401.xhp#viskartgesch" name=""><embedvar href="text/shared/00/00000004.xhp#wie"/></link></paragraph>
  <paragraph role="heading" level="2" id="hd_id3149549" l10n="U" xml-lang="en-US" oldref="3">Business data</paragraph>
  <paragraph role="paragraph" id="par_id3156027" l10n="U" xml-lang="en-US" oldref="4">Enter the contact information that you want to include on your business card.</paragraph>
  <paragraph xml-lang="en-US" oldref="17" role="note" id="par_id3155892" l10n="U">If you want to include your name on a business card, enter your name on the <emph>Private </emph>tab. Then choose a layout on the <emph>Business Cards </emph>tab that includes a name placeholder.</paragraph>
  <embed href="text/shared/optionen/01010100.xhp#datenf"/>
  <bookmark branch="hid/SW:EDIT:TP_BUSINESS_DATA:ED_COMP_EXT" xml-lang="en-US" id="bm_id3147653"/><paragraph role="heading" level="3" id="hd_id3150355" l10n="U" xml-lang="en-US" oldref="5">Company 2nd line</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3153031" xml-lang="en-US" oldref="6"><ahelp hid="SW:EDIT:TP_BUSINESS_DATA:ED_COMP_EXT" visibility="visible">Enter additional company details.</ahelp></paragraph>
  <bookmark branch="hid/SW:EDIT:TP_BUSINESS_DATA:ED_SLOGAN" xml-lang="en-US" id="bm_id3149955"/><paragraph role="heading" level="3" id="hd_id3150771" l10n="U" xml-lang="en-US" oldref="7">Slogan</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3156327" xml-lang="en-US" oldref="8"><ahelp hid="SW:EDIT:TP_BUSINESS_DATA:ED_SLOGAN" visibility="visible">Enter the slogan of your company.</ahelp></paragraph>
  <embed href="text/shared/optionen/01010100.xhp#strplzort"/>
  <bookmark branch="hid/SW:EDIT:TP_BUSINESS_DATA:ED_STATE" xml-lang="en-US" id="bm_id3145416"/><paragraph role="heading" level="3" id="hd_id3153146" l10n="U" xml-lang="en-US" oldref="9">Country</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3155449" xml-lang="en-US" oldref="10"><ahelp hid="SW:EDIT:TP_BUSINESS_DATA:ED_STATE" visibility="visible">Enter the name of the country where your business is located.</ahelp></paragraph>
  <embed href="text/shared/optionen/01010100.xhp#dateng"/>
  <bookmark branch="hid/SW:EDIT:TP_BUSINESS_DATA:ED_PHONE" xml-lang="en-US" id="bm_id3156152"/><paragraph role="heading" level="3" id="hd_id3154380" l10n="U" xml-lang="en-US" oldref="11">Phone</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3154046" xml-lang="en-US" oldref="12"><ahelp hid="SW:EDIT:TP_BUSINESS_DATA:ED_PHONE" visibility="visible">Enter your business telephone number.</ahelp></paragraph>
  <bookmark branch="hid/SW:EDIT:TP_BUSINESS_DATA:ED_MOBILE" xml-lang="en-US" id="bm_id3148990"/><paragraph role="heading" level="3" id="hd_id3158430" l10n="U" xml-lang="en-US" oldref="13">Mobile</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3156329" xml-lang="en-US" oldref="14"><ahelp hid="SW:EDIT:TP_BUSINESS_DATA:ED_MOBILE" visibility="visible">Enter your mobile telephone number.</ahelp></paragraph>
  <embed href="text/shared/optionen/01010100.xhp#datend"/>
  <bookmark branch="hid/SW:EDIT:TP_BUSINESS_DATA:ED_WWW" xml-lang="en-US" id="bm_id3153665"/><paragraph role="heading" level="3" id="hd_id3154306" l10n="U" xml-lang="en-US" oldref="15">Homepage</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3148563" xml-lang="en-US" oldref="16"><ahelp hid="SW:EDIT:TP_BUSINESS_DATA:ED_WWW" visibility="visible">Enter the address of your company's internet homepage.</ahelp></paragraph>
  <embed href="text/shared/optionen/01010100.xhp#datene"/>
  
