  <bookmark branch="hid/SVX:IMAGEBUTTON:RID_SVXFLOAT_3D:BTN_NORMALS_INVERT" xml-lang="en-US" id="bm_id3147354"/><paragraph role="heading" level="3" id="hd_id3153056" l10n="U" xml-lang="en-US" oldref="31">Invert Normals</paragraph>
