  <paragraph role="heading" level="1" id="hd_id3154689" l10n="U" xml-lang="en-US" oldref="1">Help Menu</paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3150960" xml-lang="en-US" oldref="2"><variable id="content">Choose <emph>Help - Contents</emph></variable></paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3147240" xml-lang="en-US" oldref="14"><variable id="infoanwendung">Choose <emph>Help - About <item type="productname">%PRODUCTNAME</item></emph> </variable></paragraph>
  <section id="online">
  <paragraph xml-lang="en-US" role="paragraph" id="par_id3151387" l10n="U" oldref="15">Automatically after <item type="productname">%PRODUCTNAME</item> is started first.</paragraph>
  <paragraph xml-lang="en-US" role="paragraph" id="par_id3153808" l10n="U" oldref="16">Choose <emph>Help - Registration</emph> (direct link to Web site)</paragraph>
