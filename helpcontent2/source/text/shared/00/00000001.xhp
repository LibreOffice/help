      <paragraph xml-lang="en-US" id="hd_id3152952" role="heading" level="1" l10n="U" oldref="1">Frequently-Used Buttons</paragraph>
      <section id="abbrechen">
<bookmark xml-lang="en-US" branch="hid/HID_DLGTABLE_CMDCANCEL" id="bm_id4245124" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/HID_QUERYWIZARD_CANCEL" id="bm_id7074800" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/HID_DLGIMPORT_0_CMDCANCEL" id="bm_id3147653" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/HID_FAXWIZARD_CANCEL" id="bm_id833765" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/HID0_CANCEL" id="bm_id7899105" localize="false"/>
