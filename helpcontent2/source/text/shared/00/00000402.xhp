  <paragraph l10n="U" role="paragraph" id="par_id3149259" xml-lang="en-US" oldref="547"><variable id="edit1">Choose <emph>Edit - Object</emph></variable></paragraph>
  <paragraph l10n="U" role="paragraph" id="par_id3154966" xml-lang="en-US" oldref="548"><variable id="edit2">Choose <emph>Edit - Object - Edit</emph>, also in the context menu of selected object</variable></paragraph>
