   <body>
      <section id="3dsettings">
         <paragraph xml-lang="en-US" id="par_idN1055A" role="heading" level="1" l10n="NEW"><variable id="3dtoolbar"><link href="text/shared/3dsettings_toolbar.xhp">3D-Settings</link>
</variable></paragraph>
         <paragraph xml-lang="en-US" id="par_idN1056A" role="paragraph" l10n="NEW"><ahelp hid=".">The 3D-Settings toolbar controls properties of selected 3D objects.</ahelp></paragraph>
      </section>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionToggle" id="bm_id9720450" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionToggle" id="bm_id7128513" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN10575" role="heading" level="2" l10n="NEW">Extrusion on/off</paragraph>
      <paragraph xml-lang="en-US" id="par_idN10579" role="paragraph" l10n="NEW"><ahelp hid=".">Switches the 3D effects on and off for the selected objects.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltDown" id="bm_id4860377" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltDown" id="bm_id9378337" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN10590" role="heading" level="2" l10n="NEW">Tilt Down</paragraph>
      <paragraph xml-lang="en-US" id="par_idN10594" role="paragraph" l10n="NEW"><ahelp hid=".">Tilts the selected object downwards by five degrees.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltUp" id="bm_id1682790" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltUp" id="bm_id8217413" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN105AB" role="heading" level="2" l10n="NEW">Tilt Up</paragraph>
      <paragraph xml-lang="en-US" id="par_idN105AF" role="paragraph" l10n="CHG"><ahelp hid=".">Tilts the selected object upwards by five degrees.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltLeft" id="bm_id8390758" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltLeft" id="bm_id3715628" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN105C6" role="heading" level="2" l10n="NEW">Tilt Left</paragraph>
      <paragraph xml-lang="en-US" id="par_idN105CA" role="paragraph" l10n="NEW"><ahelp hid=".">Tilts the selected object left by five degrees.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltRight" id="bm_id3985717" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/.uno:ExtrusionTiltRight" id="bm_id5299844" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN105E1" role="heading" level="2" l10n="NEW">Tilt Right</paragraph>
      <paragraph xml-lang="en-US" id="par_idN105E5" role="paragraph" l10n="NEW"><ahelp hid=".">Tilts the selected object right by five degrees.</ahelp></paragraph>
