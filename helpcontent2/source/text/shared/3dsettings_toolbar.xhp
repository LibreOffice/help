<paragraph role="paragraph" id="par_idN106B7" xml-lang="en-US" l10n="NEW"><ahelp hid=".">Opens the Extrusion Lighting window.</ahelp></paragraph>
<paragraph role="paragraph" id="par_idN106C6" xml-lang="en-US" l10n="NEW"><ahelp hid=".">Select a lighting direction.</ahelp></paragraph>
<!-- removed HID 39984 -->
<paragraph role="paragraph" id="par_idN106E1" xml-lang="en-US" l10n="NEW"><ahelp hid=".">Select a lighting intensity.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/SID_EXTRUSION_SURFACE_FLOATER" id="bm_id3682363" localize="false"/>
