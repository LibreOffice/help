<paragraph role="paragraph" id="par_idN105E5" xml-lang="en-US" l10n="NEW"><ahelp hid=".">Tilts the selected object right by five degrees.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/SID_EXTRUSION_DEPTH_FLOATER" id="bm_id1778008" localize="false"/>
