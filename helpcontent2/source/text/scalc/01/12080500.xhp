					<paragraph role="paragraph" id="par_id3145251" l10n="U" xml-lang="en-US" oldref="20">120</paragraph>
				</tablecell>
				<tablecell>
					<paragraph role="paragraph" id="par_id3149400" l10n="U" xml-lang="en-US" oldref="21">130</paragraph>
				</tablecell>
				<tablecell>
					<paragraph role="paragraph" id="par_id3150328" l10n="U" xml-lang="en-US" oldref="22">350</paragraph>
				</tablecell>
				<tablecell>
					<paragraph role="paragraph" id="par_id3155443" l10n="U" xml-lang="en-US" oldref="23">100</paragraph>
