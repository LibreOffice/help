					<paragraph role="paragraph" id="par_id3145271" l10n="U" xml-lang="en-US" oldref="17">June</paragraph>
				</tablecell>
				<tablecell>
					<paragraph role="paragraph" id="par_id3145648" l10n="U" xml-lang="en-US" oldref="18">2nd Quarter</paragraph>
				</tablecell>
			</tablerow>
			<tablerow>
				<tablecell>
					<paragraph role="paragraph" id="par_id3153876" l10n="U" xml-lang="en-US" oldref="19">100</paragraph>
				</tablecell>
