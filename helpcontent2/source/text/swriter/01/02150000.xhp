</table>

<bookmark xml-lang="en-US" branch="hid/SW:IMAGEBUTTON:DLG_INS_FOOTNOTE:BT_NEXT" id="bm_id3149631" localize="false"/>
<paragraph role="heading" id="hd_id3154630" xml-lang="en-US" level="2" l10n="U" oldref="26">Arrow right</paragraph>
<paragraph role="paragraph" id="par_id3149638" xml-lang="en-US" l10n="U" oldref="27"><ahelp hid="SW:IMAGEBUTTON:DLG_INS_FOOTNOTE:BT_NEXT">Moves to the next footnote or endnote anchor in the document.</ahelp></paragraph>
