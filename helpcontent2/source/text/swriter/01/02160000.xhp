<bookmark xml-lang="en-US" branch="hid/HID_INSERT_IDX_MRK_NEXT_SAME" id="bm_id3147747" localize="false"/>
<paragraph role="heading" id="hd_id3147736" xml-lang="en-US" level="2" l10n="U" oldref="20">End arrow to right</paragraph>
<paragraph role="paragraph" id="par_id3149829" xml-lang="en-US" l10n="U" oldref="21"><ahelp hid="HID_INSERT_IDX_MRK_NEXT_SAME">Jumps to the last index entry of the same type in the document.</ahelp></paragraph>
