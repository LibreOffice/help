<bookmark xml-lang="en-US" branch="hid/HID_INSERT_IDX_MRK_NEXT" id="bm_id3154628" localize="false"/>
<paragraph role="heading" id="hd_id3154617" xml-lang="en-US" level="2" l10n="U" oldref="24">Arrow to right</paragraph>
<paragraph role="paragraph" id="par_id3154633" xml-lang="en-US" l10n="U" oldref="25"><ahelp hid="HID_INSERT_IDX_MRK_NEXT">Jumps to the next index entry of the same type in the document.</ahelp></paragraph>
