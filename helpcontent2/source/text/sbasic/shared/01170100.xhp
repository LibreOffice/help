  <section id="control">
  <bookmark id="bm_id3153379" branch="index" xml-lang="en-US"><bookmark_value>controls; properties</bookmark_value><bookmark_value>properties; controls</bookmark_value><bookmark_value>properties; dialogs</bookmark_value><bookmark_value>dialogs; properties</bookmark_value></bookmark><bookmark branch="hid/HID_FM_PROPDLG_TABCTR" xml-lang="en-US" id="bm_id3151210"/><paragraph id="hd_id3153379" role="heading" level="1" oldref="1" l10n="U" xml-lang="en-US"><link href="text/sbasic/shared/01170100.xhp" name="Control and Dialog Properties">Control and Dialog Properties</link></paragraph>
