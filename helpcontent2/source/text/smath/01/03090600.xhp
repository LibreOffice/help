<paragraph role="paragraph" id="par_idN104E1" xml-lang="en-US" l10n="F">
<image id="img_id3153240" src="res/commandimagelist/sc_bold.png" width="0.222inch" height="0.222inch"><alt id="alt_id3153240" xml-lang="en-US">Icon</alt>
	</image></paragraph>
<bookmark xml-lang="en-US" branch="hid/HID_SMA_BOLDX" id="bm_id3146954" localize="false"/>
