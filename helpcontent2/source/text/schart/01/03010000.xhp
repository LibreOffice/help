<bookmark xml-lang="en-US" branch="hid/HID_SCH_TBI_DATA_DELETE_ROW" id="bm_id3150215" localize="false"/>
<paragraph role="heading" id="hd_id3153246" xml-lang="en-US" level="2" l10n="U" oldref="24">Delete Row</paragraph>
<paragraph role="paragraph" id="par_id3159231" xml-lang="en-US" l10n="U" oldref="26"><ahelp hid="HID_SCH_TBI_DATA_DELETE_ROW">Deletes the current row. It is not possible to delete the header row.</ahelp></paragraph>
