<bookmark xml-lang="en-US" branch="hid/HID_SCH_TBI_DATA_INSERT_COL" id="bm_id3152988" localize="false"/>
<paragraph role="heading" id="hd_id3156448" xml-lang="en-US" level="2" l10n="U" oldref="21">Insert Column</paragraph>
<paragraph role="paragraph" id="par_id3145384" xml-lang="en-US" l10n="U" oldref="23"><ahelp hid="HID_SCH_TBI_DATA_INSERT_COL">Inserts a new column after the current column.</ahelp></paragraph>
