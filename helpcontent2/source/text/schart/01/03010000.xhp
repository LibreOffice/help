</table>

<bookmark xml-lang="en-US" branch="hid/HID_SCH_TBI_DATA_INSERT_ROW" id="bm_id3154022" localize="false"/>
<paragraph role="heading" id="hd_id3146313" xml-lang="en-US" level="2" l10n="U" oldref="18">Insert Row</paragraph>
<paragraph role="paragraph" id="par_id3150297" xml-lang="en-US" l10n="U" oldref="20"><ahelp hid="HID_SCH_TBI_DATA_INSERT_ROW">Inserts a new row below the current row.</ahelp></paragraph>
