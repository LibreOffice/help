<bookmark xml-lang="en-US" branch="hid/HID_SCH_TBI_DATA_SWAP_COL" id="bm_id3149354" localize="false"/>
<paragraph role="heading" id="hd_id3146884" xml-lang="en-US" level="2" l10n="U" oldref="30">Switch Columns</paragraph>
<paragraph role="paragraph" id="par_id3157903" xml-lang="en-US" l10n="U" oldref="32"><ahelp hid="HID_SCH_TBI_DATA_SWAP_COL">Switches the current column with the column to the right.</ahelp></paragraph>
